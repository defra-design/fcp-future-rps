<nav {%- for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %}>
  <ul class="uc-claimant-navigation__list">
  {% for item in params.items %}
    {% if item.text %}
    <li class="uc-claimant-navigation__item {%- if item.active %} uc-claimant-navigation__item--active{% endif %}">
      <a class="uc-claimant-navigation__link" href="{{ item.href }}" {%- if item.active %} aria-current="true"{% endif %}>
        {{ item.text }}
      </a>
    </li>
    {% endif %}
  {% endfor %}
  </ul>
</nav>
